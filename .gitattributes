# Git LFS tracking for large data files
# Parquet files (trip data ~958MB total)
data/parquet/**/*.parquet filter=lfs diff=lfs merge=lfs -text

# ML model file (~3.5MB)
data/models/*.joblib filter=lfs diff=lfs merge=lfs -text

# GeoJSON files (large geographic data)
data/artifacts/*.geojson filter=lfs diff=lfs merge=lfs -text
